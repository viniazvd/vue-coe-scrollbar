    "node-sass": "^7.0.0",
    "poi": "^12.1.6",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.21",
    "vue-jest": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
